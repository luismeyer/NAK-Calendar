{
  "name": "calendar-formatter",
  "version": "1.0.0",
  "description": "",
  "main": "handler.js",
  "dependencies": {
    "aws-sdk": "^2.666.0",
    "date-fns": "^2.12.0",
    "ical-generator": "^1.10.0",
    "jsdom": "^16.2.2",
    "meow": "^7.0.1",
    "node-fetch": "^2.6.0",
    "node-ical": "^0.11.4"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^6.1.0",
    "jest": "^26.4.2",
    "serverless-domain-manager": "^4.2.2",
    "serverless-offline": "^6.7.0",
    "serverless-s3-local": "^0.6.5",
    "serverless-webpack": "^5.3.5",
    "webpack": "^4.44.2",
    "webpack-node-externals": "^2.5.2"
  },
  "scripts": {
    "invoke:timetable": "serverless invoke local -f timetableApi",
    "invoke:mensa": "serverless invoke local -f mensaApi",
    "invoke:bot": "serverless invoke local -f bot -p ./resources/event.json",
    "start": "serverless offline",
    "test": "jest",
    "coverage": "jest --coverage",
    "deploy": "serverless deploy",
    "setup": "node src/scripts/setup.js",
    "secrets": "gpg --symmetric --cipher-algo AES256 ./secrets/secrets.json",
    "secrets:decrypt": "gpg --decrypt --output ./secrets/secrets.json ./secrets/secrets.json.gpg",
    "meetings": "gpg --symmetric --cipher-algo AES256 ./resources/meetings.json",
    "meetings:decrypt": "gpg --decrypt --output ./resources/meetings.json ./resources/meetings.json.gpg",
    "remove:bucket": "rm -rf /tmp/nak-calendar-bucket"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luismeyer/CalendarFormatter.git"
  },
